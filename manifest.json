{
    "manifest_version": 3,
    "type": "extension",
    "name": "myplugin",
    "slug": "myplugin",
    "description": "LLM chat",
    "version": "0.0.1",
    "author": {
        "name": "dixyes",
        "link": "https://github.com/dixyes"
    },
    "repository": {
        "repo": "dixyes/myplugin",
        "branch": "latest"
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "main": "./main.mjs",
        "preload": "./preload.js",
        "renderer": "./renderer.js"
    }
}
